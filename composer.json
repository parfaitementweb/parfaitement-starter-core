{
    "name": "parfaitementweb/parfairement-starter-core",
    "description": "Core business for our Parfaitement Starter Theme",
    "type": "library",
    "require": {
        "php": "^7.1.3",
        "illuminate/view": "^5.7",
        "illuminate/support": "^5.7",
        "symfony/var-dumper": "^4.2",
        "illuminate/http": "^5.7",
        "illuminate/config": "^5.7",
        "vlucas/phpdotenv": "^3.1"
    },
    "autoload": {
        "psr-4": {
            "Parfaitement\\": "src/"
        },
        "files": [
            "src/Mix.php",
            "src/Asset.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
